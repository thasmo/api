{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "thasmo-api",
	"main": "./dist/_worker.js/index.js",
	"compatibility_date": "2025-12-30",
	"compatibility_flags": [
		"nodejs_compat",
		"global_fetch_strictly_public"
	],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"observability": {
		"enabled": true
	},
	"routes": [
		{
			"pattern": "api.thasmo.com",
			"custom_domain": true
		}
	]
}
